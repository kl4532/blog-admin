<nav class = "d-flex justify-content-center" aria-label="...">
  <ul class="pagination">
    <% if(page!==1) {%>
      <li class="page-item">
        <a class="page-link" href="/?page=1" tabindex="-1">Start</a>
      </li>
      <li class="page-item">
        <a class="page-link" href="/?page=<%= page-1 %>"><%= page-1 %></a></li>
    <% } %>
      <li class="page-item active">
        <a class="page-link" href="/?page=<%= page %>"><%= page %> <span class="sr-only"></span></a>
      </li>
    <% if(page<pages) {%>
      <li class="page-item">
        <a class="page-link" href="/?page=<%= page+1 %>"><%= page+1 %></a></li>
      <li class="page-item">
        <a class="page-link" href="/?page=<%= pages %>">End</a>
      </li>
    <% } %>
  </ul>
</nav>
